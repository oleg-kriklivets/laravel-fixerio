{
    "name": "ranium/laravel-fixerio",
    "description": "Laravel package/client for fixer.io api, a foreign exchange rates and currency conversion API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abbas Ali",
            "email": "abbas@ranium.in"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:oleg-kriklivets/fixerio-php-client.git"
        }
    ],
    "require": {
        "ranium/fixerio-php-client": "master"
    },
    "autoload": {
        "psr-4": {
            "Ranium\\LaravelFixerio\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ranium\\LaravelFixerio\\ServiceProvider"
            ],
            "aliases": {
                "Fixerio": "Ranium\\LaravelFixerio\\Facade"
            }
        }
    }
}
